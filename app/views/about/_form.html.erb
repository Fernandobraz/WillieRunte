<%= form_for @about, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :email, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :email, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :mobile_number, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :mobile_number, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :main_info, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :main_info, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :foto_file_name, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :foto_file_name, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :foto_content_type, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :foto_content_type, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :foto_file_size, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :foto_file_size, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :foto_updated_at, :class => 'control-label' %>
    <div class="controls">
      <%= f.datetime_select :foto_updated_at, :class => 'datetime_select' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                about_path, :class => 'btn' %>
  </div>
<% end %>
